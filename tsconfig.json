{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "paths": {
      "@api": ["./src/api"],
      "@api/*": ["./src/api/*"],
      "@components/*": ["./src/components/*"],
      "@components": ["./src/components"],
      "@constants/*": ["./src/constants/*"],
      "@constants": ["./src/constants"],
      "@hooks": ["./src/hooks"],
      "@hooks/*": ["./src/hooks/*"],
      "@icons/*": ["./src/icons/*"],
      "@icons": ["./src/icons"],
      "@layouts": ["./src/layouts"],
      "@layouts/*": ["./src/layouts/*"],
      "@mocks/*": ["./src/mocks/*"],
      "@mocks": ["./src/mocks"],
      "@models/*": ["./src/models/*"],
      "@models": ["./src/models"],
      "@pages/*": ["./src/pages/*"],
      "@pages": ["./src/pages"],
      "@store": ["./src/store"],
      "@store/*": ["./src/store/*"],
      "@styles/*": ["./src/styles/*"],
      "@styles": ["./src/styles"],
      "@templates/*": ["./src/templates/*"],
      "@templates": ["./src/templates"],
      "@ui-kit": ["./src/ui-kit"],
      "@ui-kit/*": ["./src/ui-kit/*"],
      "@utils/*": ["./src/utils/*"],
      "@utils": ["./src/utils"],
    },
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "jsx": "react-jsx"
  },
  "include": ["src"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
